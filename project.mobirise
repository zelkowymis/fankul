ity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.9\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay &amp;&amp; bg.type=='image')\"></div>\n    <div class=\"container\">\n        <div class=\"mbr-row mbr-jc-c\">\n            <div class=\"mbr-col-lg-6 mbr-col-md-12 md-pb mbr-m-auto mbr-col-sm-12\">\n                <div class=\"img-wrapper\">\n                    <amp-img src=\"@PROJECT_PATH@/assets/images/kulig-zakopane-ognisko-szalas.webp\" alt=\"Kuligi w Zakopanem\" width=\"506\" height=\"379.5\" layout=\"responsive\" class=\"mobirise-loader mbr-m-auto align-center\">\n                        <div placeholder=\"\" class=\"placeholder\"></div>\n                        <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/kulig-zakopane-ognisko-szalas.webp\" alt=\"Kuligi w Zakopanem\" data-aspectratio=\"0.75\">\n                    </amp-img>\n                </div>\n            </div>\n            <div class=\"mbr-col-lg-6 mbr-m-auto mbr-col-md-12 mbr-col-sm-12\">\n                <h3 class=\"main-subtitle mbr-fonts-style mbr-pb-2 align-left mbr-bold\" mbr-theme-style=\"display-4\" mbr-if=\"showmainsubTitle\" data-app-selector=\".main-subtitle\">CONTACT US</h3>\n                <h2 class=\"main-title mbr-fonts-style mbr-pb-2 align-left mbr-bold\" mbr-theme-style=\"display-5\" mbr-if=\"showmainTitle\" data-app-selector=\".main-title\">Jeli potrzebujesz wicej informacji zadzwo lub napisz do nas!</h2>\n                <p class=\"mbr-text mbr-fonts-style mbr-semibold mbr-pb-3 align-left\" mbr-theme-style=\"display-4\" mbr-if=\"showmainText\" data-app-selector=\".mbr-text\"><span style=\"font-weight: normal;\">Skorzystaj z formularza kontaktowego jeli potrzebujesz wicej informacji lub zapomniae co doda w komentarzu do zam贸wienia. Wa偶ne aby byo to z tego samego konta z kt贸rego wysane byo zam贸wienie.</span><span style=\"font-weight: normal;\"><br></span><br></p>\n                <div class=\"mbr-row mbr-jc-c\" mbr-if=\"showTitle || showText || showImage\">\n                    <div class=\"card mbr-col-lg-5 mbr-col-md-6 mbr-col-sm-12 item-border\">\n                        <div class=\"card-wrapper item-pb\">\n                            <div class=\"icon-wrapper\" mbr-if=\"showImage\">\n\n                                <div class=\"icon\">\n                                    <span class=\"iconfont-wrapper\">\n                                        <span mbr-icon=\"\" class=\"amp-iconfont mobi-mbri-phone mobi-mbri\"></span>\n                                    </span>\n                                </div>\n                            </div>\n                            <div class=\"card-box\">\n                                <h3 class=\"title mbr-fonts-style align-left mbr-semibold\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\" data-app-selector=\".title\">Numer tel.</h3>\n                                <p class=\"mbr-card-text mbr-fonts-style mbr-bold align-left\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-card-text\">\n                                    +48 693 636 136</p>\n                            </div>\n                        </div>\n                        <span mbr-text=\"\" class=\"or-text mbr-fonts-style mbr-semibold align-left\" mbr-theme-style=\"display-4\" data-app-selector=\".or-text\">lub</span>\n                    </div>\n                    <div class=\"card mbr-col-lg-7 mbr-col-md-6 mbr-col-sm-12 second-item\">\n                        <div class=\"card-wrapper\">\n                            <div class=\"icon-wrapper\" mbr-if=\"showImage\">\n\n                                <div class=\"icon\">\n                                    <span class=\"iconfont-wrapper\">\n                                        <span mbr-icon=\"\" class=\"amp-iconfont mobi-mbri-letter mobi-mbri\"></span>\n                                    </span>\n                 