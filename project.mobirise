        {
          "_name": "table3",
          "_customHTML": "<section class=\"table3\" group=\"Table\" plugins=\"amp-main, font-awesome\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"5\">\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Show Top Line\" name=\"showTopLine\">\n        <input type=\"color\" title=\" Line Color\" name=\"topLineColor\" value=\"#a2aab1\" condition=\"showTopLine\">\n        <select title=\"Cards\" name=\"cardsAmount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\" selected>3</option>\n        </select>\n                \n        <input type=\"color\" title=\"Card 1 Color\" name=\"card1Color\" value=\"#f7f7f7\">\n        <input type=\"color\" title=\"Card 2 Color\" name=\"card2Color\" value=\"#f7f7f7\" condition=\"cardsAmount==3 || cardsAmount==2\">\n        <input type=\"color\" title=\"Card 3 Color\" name=\"card3Color\" value=\"#f4f5f6\" condition=\"cardsAmount==3\">\n\n        <select title=\"Favorite Item\" name=\"favoriteItem\">\n            <option value=\"0\" selected>none</option>\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n        </select>\n\n        <input type=\"color\" title=\"Bookmark BG Color\" name=\"bookmarkBgColor\" value=\"#f7f7f7\" condition=\"favoriteItem==1||favoriteItem==2||favoriteItem==3\">\n        <input type=\"color\" title=\"Bookmark Icon Color\" name=\"bookmarkIconColor\" value=\"#f7f7f7\" condition=\"favoriteItem==1||favoriteItem==2||favoriteItem==3\">\n        <input type=\"checkbox\" title=\"Show Type\" name=\"showcardTitle\" checked>\n        <input type=\"checkbox\" title=\"Show Price\" name=\"showPrice\" checked>\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showText\" checked>\n        <input type=\"checkbox\" title=\"Show List String\" name=\"showListString\" checked>\n        <input type=\"range\" inline title=\"String Count\" name=\"listGrow\" min=\"1\" max=\"5\" step=\"1\" value=\"4\" condition=\"showListString\">\n        <input type=\"checkbox\" title=\"Show Icons\" name=\"showIcons\" condition=\"showListString\">\n        <input type=\"color\" title=\"Card 1 Counter Color\" name=\"Card1CounterColor\" value=\"#bbc3cc\" condition=\" showIcons && showListString\">\n        <input type=\"color\" title=\"Card 2 Counter Color\" name=\"Card2CounterColor\" value=\"#af9f8c\" condition=\"(cardsAmount==3 || cardsAmount==2) && showIcons && showListString\">\n        <input type=\"color\" title=\"Card 3 Counter Color\" name=\"Card3CounterColor\" value=\"#bbc3cc\" condition=\"cardsAmount==3 && showIcons && showListString\">\n        <input type=\"range\" inline title=\"Icon Size\" name=\"iconSize\" min=\"0.5\" max=\"1.5\" step=\"0.1\" value=\"1\" condition=\"showIcons\">\n        <input type=\"checkbox\" title=\"Show Underline\" name=\"showUnderline\" checked>\n        \n        <input type=\"color\" title=\"Underline Color\" name=\"underlineColor\" value=\"#616161\" condition=\"showUnderline\">\n\n        <input type=\"checkbox\" title=\"Show Buttons\" name=\"showButtons\" checked>\n\n        <input type=\"checkbox\" title=\"Show Text After Cards\" name=\"textAfterCards\" checked>\n\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/kuligi-zima-zakopane-143.webp\" selected>\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay && bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div class=\"container\">\n        <div class=\" mbr-row mbr-justify-content-center\">\n            <div class=\" main-title mbr-col-sm-12  mbr-col-md-12  mbr-col-lg-12\">\n                <h2 class=\"mbr-section-title mbr-fonts-style mbr-black mbr-bold align-center mbr-mb-8\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title, .line\">Zimowy Kulig Zakopane cena pakietu</h2>\n                <div class=\"line align-center\">\n                    <div mbr-if=\"showTopLine \" class=\"line-bottom\"></div>\n                </div>\n            </div>\n        </div>\n        <div class=\" mbr-row mbr-justify-content-center mbr-mt-30\">\n            <div class=\"card mbr-col-sm-12 mbr-col-md-12\" mbr-class=\"{\n                              'mbr-col-lg-6': cardsAmount == 2,                                                \n                             'mbr-col-lg-4': cardsAmount == 3,\n                             'last-child': cardsAmount == 1}\">\n\n\n                <div class=\"card-wrapper\">\n\n                    <div class=\"card-box first-card\">\n                        <div class=\"hot\" mbr-class=\"{'display': favoriteItem == 1}\"> </div>\n                        <span mbr-icon mbr-class=\"{'display': favoriteItem == 1}\" class=\"mbr-iconfont ico fa-star fa\"></span>\n\n                        <div class=\"favorite\" mbr-class=\"{'favorite-active': favoriteItem == 1}\">\n\n\n                        </div>\n\n\n\n                        <p class=\"card-title first-card-title mbr-mb-8 mbr-fonts-style mbr-light mbr-black align-center\" mbr-theme-style=\"display-6\" mbr-if=\"showcardTitle\" data-app-selector=\".first-card-title\">za osobę</p>\n\n\n                        <h3 class=\"price mbr-fonts-style mbr-mb-8 mbr-black mbr-bold align-center l-h-12\" mbr-theme-style=\"display-2\" mbr-if=\"showPrice\" data-app-selector=\".price\"><span class=\"pricecount\">120 zł</span></h3>\n\n\n                        <p class=\"mbr-text first-card-text text-card mbr-fonts-style mbr-light mbr-black align-center\" mbr-theme-style=\"display-5\" mbr-if=\"showText\" data-app-selector=\".first-card-text, .mbr-section-btn\">dorośli</p>\n                        <div class=\"mbr-text mbr-black counter-container mbr-fonts-style  mbr-mt-30\" mbr-if=\"showListString\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-text\" mbr-list-grow=\"listGrow\" mbr-list>\n                            <div class=\"list-item first-card-item item-underline d-flex mbr-align-items-center\">\n                                <div class=\"card-img mbr-justify-content-center mbr-align-items-center\" mbr-if=\"showIcons\">\n                                    <div class=\"icon-wrap\">\n                                        <span mbr-icon class=\"mbr-iconfont fa-check fa\"></span>\n                                    </div>\n                                </div>\n                                <p class=\"align-center first-card-item item-string mbr-text mbr-fonts-style align-center\" data-app-selector=\".first-card-item\">\n                                    Transport\n                                </p>\n                            </div>\n                            <div class=\"list-item first-card-item item-underline d-flex mbr-align-items-center\">\n                                <div class=\"card-img mbr-justify-content-center mbr-align-items-center\" mbr-if=\"showIcons\">\n                                    <div class=\"icon-wrap\">\n                                        <span mbr-icon class=\"mbr-iconfont fa-check fa\"></span>\n                                    </div>\n                                </div>\n                                <p class=\"align-center first-card-item item-string mbr-text mbr-fonts-style align-center\" data-app-selector=\".first-card-item\">\n                                    KULIG\n                                </p>\n                            </div>\n                            <div class=\"list-item first-card-item item-underline d-flex mbr-align-items-center\">\n                                <div class=\"card-img mbr-justify-content-center mbr-align-items-center\" mbr-if=\"showIcons\">\n                                    <div class=\"icon-wrap\">\n                                        <span mbr-icon class=\"mbr-iconfont fa-check fa\"></span>\n                                    </div>\n                                </div>\n                                <p class=\"align-center first-card-item item-string mbr-text mbr-fonts-style align-center\" data-app-selector=\".first-card-item\">\n                                    Poczęstunek bez limitów\n                                </p>\n                            </div>\n                        <div class=\"list-item first-card-item item-underline d-flex mbr-align-items-center\">\n                                <div class=\"card-img mbr-justify-content-center mbr-align-items-center\" mbr-if=\"showIcons\">\n                                    <div class=\"icon-wrap\">\n                                        <span mbr-icon class=\"mbr-iconfont fa-check fa\"></span>\n                                    </div>\n                                </div>\n                                <p class=\"align-center first-card-item item-string mbr-text mbr-fonts-style align-center\" data-app-selector=\".first-card-item\">\n                                    Ubezpieczenie\n   